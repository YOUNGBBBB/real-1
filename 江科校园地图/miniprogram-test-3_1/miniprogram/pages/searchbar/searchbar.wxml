<!--pages/searchbar/searchbar.wxml-->
<view class="page">
    
    <view class="page__bd">
        <view class="weui-search-bar" bindtap="showInput">
            <view class="weui-search-bar__form">
                    <icon class="weui-icon-search_in-box" type="search" size="14"></icon>
                <view  class="weui-search-bar__input"    >
                    </view>
               
                <label class="weui-search-bar__label" hidden="{{inputShowed}}">
                    <icon class="weui-icon-search" type="search" size="14"></icon>
                    <view class="weui-search-bar__text">搜索地点</view>
                </label>
            </view>
           
        </view>
        
    </view>
</view>
